@{
    var conexionDB = Database.Open("ObligacionesOU");
    var datos = conexionDB.Query ("SELECT * FROM OU"); 
}

<!DOCTYPE html>

<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title>BaseOU</title>
        <script src="js/jquery110.js"></script>
        <script src="js/grillasok.js"></script>
    </head>
    <body>
        <h1>Base de Datos Obligaciones Urbanisticas</h1>
        <div id="Contenedor">
            <div id="MenuLateral">
                <ul>
                    <li><a href="BaseOU">Obligaciones Urbanisticas</a></li>
                    <li><a href="InsertarOU">Insertar</a></li>
                    <li><a href="ActualizarOU">Actualizar</a></li>
                    <li><a href="EliminarOU">Eliminar</a></li>
                    <li><a href="ExportarOU">Exportar</a></li>
                </ul>
            </div>
            <section>
                <div id="ObligacionesOU">
                    <table border="1">
                        <tr>
                            <th>Fecha Recibido</th>
                            <th>Número Tramite</th>
                            <th>Nombre Solicitante</th>
                            <th>Nombre Edificio o Dirección</th>
                            <th>Cobama</th>
                            <th>Barrio</th>
                            <th>Matricula</th>
                            <th>Número OU</th>
                            <th>Fecha Respuesta</th>
                            <th>Nombre Funcionario</th>
                            <th>V/R LOTE 100</th>
                            <th>Vr. Const al 50%</th>
                            <th>Vr. M2 Construído L/C</th>
                            <th>Observaciones</th>
                        </tr>

                             @foreach (var fila in datos){
                                 <tr>
                                     <td>@fila.FechaRecibido</td>
                                     <td>@fila.NumeroTramite</td>
                                     <td>@fila.NombreSolicitante</td>
                                     <td>@fila.NomEdificioDir</td>
                                     <td>@fila.Cobama</td>
                                     <td>@fila.Barrio</td>
                                     <td>@fila.Matricula</td>
                                     <td>@fila.NumeroOU</td>
                                     <td>@fila.FechaRespuesta</td>
                                     <td>@fila.NombreFuncionario</td>
                                     <td>@fila.VRLote100</td>
                                     <td>@fila.VrConstal50</td>
                                     <td>@fila.Vrm2construidoLC</td>
                                     <td>@fila.Observaciones</td>
                                 </tr>
                             }
                    </table>
                   
                </div>

                <button>Enviar
                  
                </button>
            </section>

            <div id="contenedordinamico">
                <div id="grilladedatos"></div>
                <button id="save">Guardar</button>
                <div id="log" style="overflow:auto; height:200px; background-color:#f2f2f2; padding:5px"></div>           
            </div>
        </div>

    </body>
</html>
